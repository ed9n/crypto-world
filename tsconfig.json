{
    "compilerOptions": {
      "outDir": "./public/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "paths": {
        "components/*": ["./src/components/*"],
        "requests/*": ["./src/requests/*"],
        "keys/*": ["./src/keys/*"],
        "pages/*": ["./src/pages/*"]
      }
    }
  }